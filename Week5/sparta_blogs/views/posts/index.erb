<h1>Welcome to our BlogSite!</h1>
<!-- Ice Cream Cone: Embeds ruby code that will run but won't be displayed -->
<% %>
<!-- Ice Cream Cone With Flake: Embeds ruby code that will be displayed -->
<%= %>

<!-- Displays the Title Variable -->
<h2><%= @title %></h2>

<!-- Loops through posts -->
<% @posts.each do |post| %>
  <div class="">
    <h3><a href="/<%= post[:id]%>"><%= post[:title] %></a></h3>
    <p><%= post[:body] %></p>
  </div>
<% end %>
